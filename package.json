{
  "name": "basic-testing",
  "version": "1.0.0",
  "description": "Task for mastering unit-tests with Jest, Node.js and Typescript",
  "author": "AlreadyBored",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest --silent",
    "test:verbose": "jest",
    "test:01-simple-tests": "jest src/01-simple-tests/index.test.ts",
    "test:02-table-tests": "jest src/02-table-tests/index.test.ts",
    "test:03-error-handling-async": "jest src/03-error-handling-async/index.test.ts",
    "test:04-test-class": "jest src/04-test-class/index.test.ts",
    "test:05-partial-mocking": "jest src/05-partial-mocking/index.test.ts",
    "test:06-mocking-node-api": "jest src/06-mocking-node-api/index.test.ts",
    "test:07-mocking-lib-api": "jest src/07-mocking-lib-api/index.test.ts",
    "test:07-mocking-lib-api-2": "jest src/07-mocking-lib-api/index.spec.ts",
    "test:08-snapshot-testing": "jest src/08-snapshot-testing/index.test.ts",
    "build": "tsc"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "lodash": "^4.17.21"
  }
}
